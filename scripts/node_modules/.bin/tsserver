#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/op/Herd/lnear/node_modules/.pnpm/typescript@5.4.5/node_modules/typescript/bin/node_modules:/Users/op/Herd/lnear/node_modules/.pnpm/typescript@5.4.5/node_modules/typescript/node_modules:/Users/op/Herd/lnear/node_modules/.pnpm/typescript@5.4.5/node_modules:/Users/op/Herd/lnear/node_modules/.pnpm/node_modules:/Users/op/Herd/lnear/node_modules:/Users/op/Herd/node_modules:/Users/op/node_modules:/Users/node_modules:/node_modules:/Users/op/Herd/lnear/scripts/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/op/Herd/lnear/node_modules/.pnpm/typescript@5.4.5/node_modules/typescript/bin/node_modules:/Users/op/Herd/lnear/node_modules/.pnpm/typescript@5.4.5/node_modules/typescript/node_modules:/Users/op/Herd/lnear/node_modules/.pnpm/typescript@5.4.5/node_modules:/Users/op/Herd/lnear/node_modules/.pnpm/node_modules:/Users/op/Herd/lnear/node_modules:/Users/op/Herd/node_modules:/Users/op/node_modules:/Users/node_modules:/node_modules:/Users/op/Herd/lnear/scripts/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../typescript/bin/tsserver" "$@"
fi
