{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "aerni/laravel-spotify": "^2.2",
        "graham-campbell/github": "^12.6",
        "laravel/framework": "^11.7",
        "laravel/tinker": "^2.9",
        "lnear/html": "^1.1",
        "lnear/json": "^0.0.2",
        "lnear/marco": "^0.0.7",
        "spatie/commonmark-shiki-highlighter": "^2.4",
        "spatie/laravel-data": "^4.6",
        "spatie/laravel-typescript-transformer": "^2.4.1",
        "tempest/highlight": "^2.7"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23.1",
        "laravel/breeze": "^2.0.3",
        "laravel/pint": "^1.15.3",
        "laravel/sail": "^1.29.1",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.1.1",
        "pestphp/pest": "^2.34.7",
        "pestphp/pest-plugin-laravel": "^2.4",
        "spatie/laravel-ignition": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "": "ddd/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "functions/index.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
